      if (isNotUndef(tabPanel_tabs[i].items.warning))
        tabPanel_tabs[i].items.warning()
  }

  //? While repeatIndex is defined, the window will be opened again
  repeatIndex = undefined
  win.show()

  return {
    repeatIndex: repeatIndex,
    tabs: tabPanel_tabs
  }

}

















